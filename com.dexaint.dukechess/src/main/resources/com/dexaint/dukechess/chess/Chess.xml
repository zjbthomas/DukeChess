	<chess name="Assassin">
		<styles>
			<!-- frontMoveStyle -->
			<style>
				<action>Move</action>
				<starter>1</starter>
				<targets>
					<target>
						<destination>UU</destination>
						<movement>JumpSlide</movement>
					</target>
					<target>
						<destination>DDRR</destination>
						<movement>JumpSlide</movement>
